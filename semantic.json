{
  "base": "semantic",
  "paths": {
    "source": {
      "config": "../src/less/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../dist/semantic",
      "themes": "../dist/semantic/themes/",
      "uncompressed": "../dist/semantic/components/",
      "compressed": "../dist/semantic/components/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": true,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "card",
    "container",
    "header",
    "grid",
    "icon",
    "item",
    "menu",
    "reset",
    "segment",
    "sidebar"
  ],
  "version": "2.2.11"
}